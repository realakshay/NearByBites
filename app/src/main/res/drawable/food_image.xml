<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#553D2B" /> <!-- Wooden table color -->
        </shape>
    </item>
    <item android:top="20dp" android:bottom="120dp" android:left="40dp" android:right="40dp">
        <shape android:shape="oval">
            <solid android:color="#E8B056" /> <!-- Food color (burger bun) -->
            <stroke android:color="#CEA052" android:width="4dp" />
        </shape>
    </item>
    <item android:top="110dp" android:bottom="70dp" android:left="60dp" android:right="60dp">
        <shape android:shape="rectangle">
            <solid android:color="#8BC34A" /> <!-- Lettuce color -->
        </shape>
    </item>
    <item android:top="150dp" android:right="120dp" android:left="40dp">
        <shape android:shape="oval">
            <solid android:color="#B52B2B" /> <!-- Tomato color -->
            <size android:width="15dp" android:height="15dp" />
        </shape>
    </item>
    <item android:top="180dp" android:left="130dp">
        <shape android:shape="oval">
            <solid android:color="#B52B2B" /> <!-- Tomato color -->
            <size android:width="15dp" android:height="15dp" />
        </shape>
    </item>
</layer-list>